<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/camera"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Camera"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/location"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Location"
                    android:layout_weight="1" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/audio"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="AUDIO"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/bodysensor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="BODY SENSOR"
                    android:layout_weight="1" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/storage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="STORAGE GROUP"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/rstorage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="READ STORAGE"
                    android:layout_weight="1" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/wstorage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="WRITE STORAGE"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/calender"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="CALENDER GROUP"
                    android:layout_weight="1" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/rcalendar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Read STORAGE"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/wcalendar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Write CALENDER"
                    android:layout_weight="1" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/contact"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Contact GROUP"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/gaccounts"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="GET Accounts"
                    android:layout_weight="1" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/rcontact"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Read Contact"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/wcontact"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Write Contact"
                    android:layout_weight="1" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/smsgroup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="SMS GROUP"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/sms"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="SMS"
                    android:layout_weight="1" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/rsms"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="READ SMS"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/sendsms"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="SEND SMS"
                    android:layout_weight="1" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/receivesms"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="RECEIVE SMS"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/receive_mms"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="RECEIVE MMS"
                    android:layout_weight="1" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/phonegroup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="PHONE GROUP"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/sip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="SIP"
                    android:layout_weight="1" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/readphonestate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="READ_PHONE_STATE"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/call_phone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="CALL PHONE"
                    android:layout_weight="1" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/readcalllog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="READ CALL LOG"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/writecalllog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="WRITE CALL LOG"
                    android:layout_weight="1" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/add_voice_mail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ADD VOICE MAIL"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/multi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="MULTI"
                    android:layout_weight="1" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp">
                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/receivewap"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="RECEIVE WAP PUSH"
                    android:layout_weight="1"/>

                <Button
                    android:layout_marginHorizontal="5dp"
                    android:id="@+id/call_log_group"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="CALL LOG"
                    android:layout_weight="1"/>


            </LinearLayout>


        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>